{
  "scripts": {
    "build": "snowpack build",
    "lint:scripts": "eslint src --ext .js,json,.ts",
    "lint:scripts:watch": "esw -w --clear src --ext .js,json,.ts",
    "lint:styles": "stylelint **/*.{css,html,js,md,svelte,svg,ts}",
    "lint:styles:watch": "chokidar --silent \"**/*.{css,html,js,md,svelte,svg,ts}\" -c \"node -e \"process.stdout.write('\\033c\\033[3J')\" && npm run lint:styles --silent\"",
    "lint:svelte": "svelte-check",
    "lint:svelte:watch": "chokidar --silent \"**/*.svelte\" -c \"node -e \"process.stdout.write('\\033c\\033[3J')\" && npm run lint:svelte\"",
    "start": "snowpack dev",
    "test": "jest",
    "test:watch": "npm run test -- --watch --detectOpenHandles",
    "update": "ncu -u"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-svelte": "^3.5.2",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.20",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/node": "^14.14.34",
    "@types/puppeteer": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "eslint-watch": "^7.0.0",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "npm-check-updates": "^11.3.0",
    "postcss": "^8.2.8",
    "postcss-color-function": "^4.1.0",
    "postcss-contrast": "0.0.4",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.1",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "puppeteer": "^8.0.0",
    "snowpack": "^3.0.13",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "svelte-check": "^1.2.5",
    "svelte-preprocess": "^4.6.9",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "svelte": "^3.35.0"
  }
}
